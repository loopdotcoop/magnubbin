{
  "name": "magnubbin",
  "version": "0.0.14",
  "description": "A super-rapid 3D prototyping system",
  "main": "build/magnubbin.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "usage"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
    "ookonsole": "latest",
    "oo3d": "latest"
  },
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "make": "node bin/akaybe-build --plugin --npm index",
    "index": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js node_modules/ookonsole/build/ookonsole.js node_modules/oo3d/node_modules/nwang/build/nwang.js node_modules/oo3d/build/oo3d.js build/magnubbin.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopdotcoop/magnubbin.git"
  },
  "keywords": [
    "CoffeeScript",
    "3D",
    "Prototyping",
    "Modelling"
  ],
  "author": "Rich Plastow <rich@loop.coop> (http://loop.coop/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopdotcoop/magnubbin/issues"
  },
  "homepage": "http://magnubbin.loop.coop/"
}
